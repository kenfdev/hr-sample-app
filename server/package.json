    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/faker": "^4.1.11",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@types/supertest": "^2.0.11",
    "@types/yargs": "^17.0.4",
    "jest": "^27.3.1",
    "nodemon": "^2.0.14",
    "supertest": "^6.1.6",
